<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<!-- Dependency injection -->

	<bean id="employee" class="cg.feb18.spring.lab.model.Employee">
		<property name="employeeId" value="101"></property>
		<property name="employeeName" value="Saakshi"></property>
		<property name="salary" value="50000"></property>
		<property name="bussinessUnit" value="Software Engineer"></property>
		<property name="age" value="20"></property>

	</bean>

	<bean id="sbu" class="cg.feb18.spring.lab.model.Sbu">
		<property name="sbuCode" value="102"></property>
		<property name="sbuName" value="shweta"></property>
		<property name="sbuHead" value="developer"></property>
	</bean>


</beans>